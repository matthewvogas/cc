{
  "name": "codecoco",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.1",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "daisyui": "^3.1.6",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "next-auth": "^4.22.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "swiper": "^9.4.1",
    "swr": "^2.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "xlsx": "^0.18.5"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} ./prisma/seed.ts"
  },
  "volta": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSameLine": true,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2,
    "semi": false
  }
}
